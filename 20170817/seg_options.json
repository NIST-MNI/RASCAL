{
  "initial_register": {
    "options":   "bestlinreg_new",
    "use_mask":   false,
    "type":      "-lsq9",
    "objective": "-xcorr"
  },

  "initial_local_register":  {
    "type":     "ants",
    "bbox":     true,
    "use_mask": true,
    
    "options":  {
        "cost_function_par":          "1,32,regular,0.5",
        "transformation":             "affine[ 0.5 ]",
        "winsorize-image-intensities":"[0.005,0.995]"
    }
  },
  
  "non_linear_pairwise": false,
  "non_linear_register": true,
  "non_linear_register_ants": true,

  "simple_fusion": false,
  "non_linear_register_level": 1,
  
  "resample_order": 4,
  "resample_baa": true,
  "library_preselect":-1,
  
  "fuse_options":
    {
      "patch": 3,
      "search": 2,
      "threshold": 0.0,
      
      "gco":false,
      "gco_wlabel": 0.01,
      "gco_wdata":  200.0,
      "gco_epsilon": 0.00001
    },


    "non_linear_register_options": {
          "conf":   {"16":200,"8":200,"4":100,"2":50, "1": 20  },
          "blur":   {"16":16 ,"8":8  ,"4":4  ,"2":2,  "1": 1   },
          "shrink": {"16":16 ,"8":8  ,"4":4  ,"2":2,  "1": 1   },

          "transformation":         "SyN[ .25, 2.0 , 0.2 ]",
          "use_histogram_matching":  true,
          "cost_function":          "CC",
          "cost_function_par":      "1,3,Regular,1.0",
          "winsorize-image-intensities": { "low":1,"high":99 }
    },
    
    "post_filters": {
     "normalize": true,
  
     "nuyl": false, 
     "denoise": false, 
     "denoise_beta": 0.5, 
     "denoise_search": 1, 
     "denoise_patch": 1
    },
    
    "segment_symmetric":true
    
}
